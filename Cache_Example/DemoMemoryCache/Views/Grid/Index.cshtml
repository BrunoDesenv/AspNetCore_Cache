@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

@foreach (var user in Model)
{
    <div class="grid grid-pad">
        <div class="col-1-1">
            <div class="content">
                <h3>
                    Code: @user.Id
                    Name: @user.Name
                </h3>
            </div>
        </div>
    </div>
}
